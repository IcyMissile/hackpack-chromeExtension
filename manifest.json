{
"manifest_version": 2,
    "name": "Smile Surgeries",
    "description": "donate to operation smile surgeries",
    "version": "1.0",

   "browser_action": {
    "default_popup": "popup.html"
   },

    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}